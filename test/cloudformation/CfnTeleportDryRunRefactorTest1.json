{
  "Resources": {
    "DryRunRefactorBucket": {
      "Type": "AWS::S3::Bucket",
      "Properties": {
        "BucketName": {
          "Fn::Sub": "${AWS::AccountId}-cfn-teleport-dryrun-refactor"
        },
        "Tags": [
          {
            "Key": "ApplicationName",
            "Value": "cfn-teleport-test"
          }
        ]
      },
      "UpdateReplacePolicy": "Delete",
      "DeletionPolicy": "Delete"
    },
    "DryRunRefactorTable": {
      "Type": "AWS::DynamoDB::Table",
      "Properties": {
        "AttributeDefinitions": [
          {
            "AttributeName": "id",
            "AttributeType": "S"
          }
        ],
        "KeySchema": [
          {
            "AttributeName": "id",
            "KeyType": "HASH"
          }
        ],
        "ProvisionedThroughput": {
          "ReadCapacityUnits": 5,
          "WriteCapacityUnits": 5
        },
        "TableName": "cfn-teleport-dryrun-refactor",
        "Tags": [
          {
            "Key": "ApplicationName",
            "Value": "cfn-teleport-test"
          }
        ]
      },
      "UpdateReplacePolicy": "Delete",
      "DeletionPolicy": "Delete"
    }
  }
}
